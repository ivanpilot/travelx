<h1>Sign In</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for User.new, :url => sign_in_path do |f|%>
      <%= f.label :email, class: "form-group" %>
      <%= f.text_field :email, class: "form-control", autocomplete:"off" %><br>

      <%= f.label :password, class: "form-group" %>
      <%= f.password_field :password, class: "form-control" %><br>

      <%= f.submit 'Sign In', class: "btn btn-primary"%>
    <% end %>
  </div>
</div>
