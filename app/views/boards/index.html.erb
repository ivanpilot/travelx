<h3>Create a new board</h3>
<%= form_for Board.new do |f|%>
  <%= f.label :title %>
  <%= f.text_field :title%>
  <%= f.submit "Create", class: "btn btn-primary"%>
<% end %>


<h3>Existing boards</h3>
<ul>
  <% @boards.each do |board| %>
    <li><%= link_to board.title, board_path(board) %></li>
  <% end %>
</ul>
